<div class="table-responsive">
	<table class="table table-striped table-hover">
		<thead>
			<tr>
			  <th>Department Name</th>
			  <th>Action</th>
			</tr>
		</thead>
	  <tbody>
	  	<% obj.each do |department| %>
				<tr>
					<td><%= department.name %></td>
					<td><%= link_to "Edit", edit_department_path(department) %> | <%= link_to "Delete", department_path(department), method: :delete, data: { confirm: "Are you sure you want to delete this Department?"}  %></td>
				</tr>
			<% end %>
	  </tbody>
	</table>
</div>
