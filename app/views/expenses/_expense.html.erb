<div class="table-responsive">
	<table class="table table-striped table-hover">
		<thead>
			<tr>
			  <th>Job Id</th>
			  <th>Description</th>
			  <th>Amount</th>
			  <th>Action</th>
			</tr>
		</thead>
	  <tbody>
	  	<% obj.each do |expense| %>
				<tr>
					<td><%= expense.job_id %></td>
					<td><%= expense.description %></td>
					<td><%= expense.amount %></td>
					<td><%= link_to "Edit", edit_expense_path(expense) %> | <%= link_to "Delete", expense_path(expense), method: :delete, data: { confirm: "Are you sure you want to delete this Expense?"}  %></td>
				</tr>
			<% end %>
	  </tbody>
	</table>
</div>
